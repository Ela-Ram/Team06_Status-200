				{
					"name": "Update Batch",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"// Send a get request to first fetch the batch by batch id.\r",
									"let scenario = pm.variables.get(\"scenario\"); \r",
									"let get_endpoint = \"/batches/batchId/{{BatchId}}\";\r",
									"let baseUrl = pm.environment.get(\"Base_URL\");\r",
									"let fullUrl = '';\r",
									"\r",
									"if (scenario != \"update batch with invalid batchId\") {    \r",
									"    let batchId = pm.collectionVariables.get(\"BatchId\"); \r",
									"    console.log(\"batchId inside put-pre-request:\" + batchId);\r",
									"    if (batchId) {\r",
									"        // Replace the placeholder {{BatchId}} dynamically\r",
									"        fullUrl = `${baseUrl}${get_endpoint}`.replace(\"{{BatchId}}\", batchId);\r",
									"        console.log(\"full url:\" + fullUrl);\r",
									"    } else {\r",
									"        console.log(\"❌ BatchId is not set. Request URL will be incorrect.\");\r",
									"    }\r",
									"\r",
									"    pm.sendRequest({\r",
									"        url: fullUrl,\r",
									"        method: \"GET\",\r",
									"        header: {\r",
									"            \"Content-Type\": \"application/json\",\r",
									"            \"Authorization\": \"Bearer \" + pm.collectionVariables.get(\"authToken\") \r",
									"        }\r",
									"    }, function (err, res) {\r",
									"        if (!err && res.code === 200) {\r",
									"            let existingBatch = res.json();\r",
									"            console.log(\"get batch successful\");\r",
									"            console.log(existingBatch);\r",
									"            \r",
									"            // Update only fields that are passed in from json file\r",
									"            let batchName = pm.variables.get(\"batchName\");\r",
									"            if (batchName) {\r",
									"                existingBatch.batchName = batchName;\r",
									"            }\r",
									"        \r",
									"            let batchDescription = pm.variables.get(\"batchDescription\");\r",
									"            if (batchDescription) {\r",
									"                existingBatch.batchDescription = batchDescription;\r",
									"            }\r",
									"\r",
									"            let batchStatus = pm.variables.get(\"batchStatus\");\r",
									"            if (batchStatus) {\r",
									"                existingBatch.batchStatus = batchStatus;\r",
									"            }\r",
									"\r",
									"            let batchNoOfClasses = pm.variables.get(\"batchNoOfClasses\");\r",
									"            if (batchNoOfClasses) {\r",
									"                existingBatch.batchNoOfClasses = batchNoOfClasses;\r",
									"            }\r",
									"\r",
									"            let programId = pm.variables.get(\"programId\");\r",
									"            if (programId) {\r",
									"                existingBatch.programId = programId;\r",
									"            }\r",
									"\r",
									"            let programName = pm.variables.get(\"programName\");\r",
									"            if (programName) {\r",
									"                existingBatch.programName = programName;\r",
									"            }\r",
									"\r",
									"            pm.variables.set(\"updatedBatch\", JSON.stringify(existingBatch)); // Store this for use in PUT body\r",
									"            console.log(pm.variables.get(\"updatedBatch\")); \r",
									"        }\r",
									"    });\r",
									"\r",
									"    // Update BatchId from collection variable for PUT request\r",
									"    let endpoint = pm.variables.get(\"endpoint\");\r",
									"    console.log(\"Endpoint is:\" + endpoint);    \r",
									"    if (batchId) {\r",
									"        // Replace the placeholder {{BatchId}} dynamically\r",
									"        fullUrl = `${baseUrl}${endpoint}`.replace(\"{{BatchId}}\", batchId);\r",
									"        pm.request.url = fullUrl;\r",
									"        console.log(\"full url for update is: \" + fullUrl);\r",
									"    } else {\r",
									"        console.log(\"❌ BatchId is not set. Request URL will be incorrect.\");\r",
									"    }\r",
									"}\r",
									"\r",
									"\r",
									"\r",
									"\r",
									"\r",
									"\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
